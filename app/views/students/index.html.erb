<h1> Student List </h1>

<table>
    <thead>
        <tr>
            <th> First Name </th>
            <th> Last Name </th>
            <th> Email </th>
            <th> City </th>
        </tr>
    </thead>
    <tbody>
        <% @students.each do |student| %>
            <tr>
                <td> <%= student.first_name %> </td>
                <td> <%= student.last_name %> </td>
                <td> <%= student.email %> </td>
                <td> <%= student.city %> </td>
            </tr>
        <% end %>
    </tbody>
</table>